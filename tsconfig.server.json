{
  "compilerOptions": {
    "outDir": "build-server",
    "module": "commonjs",
    "target": "es6",
    "lib": ["es6", "dom"],
    "noImplicitAny": true,
    "sourceMap": true,
    "jsx": "react",
    "moduleResolution": "node",
    "baseUrl": "src",
  },
  "rules": {
    "no-console": 0,
  },
  "jsRules": {
    "no-console": false //Disable for javascript
  }
}
